// Test Repeat-When-Fixed without default case
// Tests when no case matches and there's no fixed case

run {
    num x = 99;
    
    repeat [x] {
        when [1] {
            @print["Case one"];
        }
        when [2] {
            @print["Case two"];
        }
        when [3] {
            @print["Case three"];
        }
    }
    
    @print["After repeat statement"];
};
