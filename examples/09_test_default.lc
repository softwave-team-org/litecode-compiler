// Test Repeat-When-Fixed with default case
// Tests the fixed (default) case

run {
    num x = 99;
    
    repeat [x] {
        when [1] {
            @print["Case one"];
        }
        when [2] {
            @print["Case two"];
        }
        when [3] {
            @print["Case three"];
        }
        fixed {
            @print["Default case - value was 99"];
        }
    }
};
